#
# gcc compilation aliases: c89, c99, dubug mode, release mode
alias gd='gcc -ansi -pedantic-errors -Wall -Wextra -g'
alias gc='gcc -ansi -pedantic-errors -Wall -Wextra -DNDEBUG -03'
alias gd9='gcc -std=c99 -pedantic-errors -Wall -Wextra -g'
alias gc9='gcc -std=c99 -pedantic-errors -Wall -Wextra -DNDEBUG -03'
#
# Valgrind alias
alias vlg='valgrind --leak-check=yes --track-origins=yes'

#Added on worksheet 7
alias gd_c='gcc -ansi -pedantic-errors -Wall -Wextra -g -c'
